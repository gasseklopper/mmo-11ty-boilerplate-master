<ul>
	{% for post in postslist %}
		<li>
			<strong>
				<a href="{{ post.url | url}}">
					{{ post.data.title }}
					{{ post.data.title2 }}
					<img src="{{ post.data.featuredImage | url }}" alt="{{ post.data.featuredImage }}">
				</a>
			</strong>
			-
			<span>
				{{ post.data.description}}
			</span>

		</li>
	{% endfor %}
</ul>