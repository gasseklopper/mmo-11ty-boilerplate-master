{
  "name": "mmo-11ty-boilerplate-master",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "npx eleventy",
    "del:assets": "rimraf ./src/compiled-assets",
    "del:dist": "rimraf ./site",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "dev:site": "npx eleventy --serve",
    "prod2": "npm-run-all del:dist del:assets build:assets build:site",
    "serve:prod": "serve ./dist/",
    "dev": "concurrently --kill-others \"npm run dev:assets\" \"npm run dev:site\"",
    "prod": "concurrently \"npm run build:assets\" \"npm run build:site\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "autoprefixer": "^10.2.3",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "cssnano-webpack-plugin": "^1.0.3",
    "fibers": "^5.0.0",
    "gsap": "^3.6.0",
    "headroom.js": "^0.12.0",
    "locomotive-scroll": "^4.0.6",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "mini-css-extract-plugin": "^1.3.4",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "serve": "^11.3.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  },
  "browserslist": [
    "defaults",
    "ie 11",
    "not ie_mob 11",
    "not baidu >= 7.12",
    "not and_qq >= 1.2",
    "not op_mini all"
  ]
}
