{
  "name": "mmo-11ty-boilerplate-master",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "ELEVENTY_ENV=production npx eleventy",
    "del:assets": "rimraf ./src/compiled-assets",
    "del:dist": "rimraf ./site",
    "devsss": "npm-run-all dev:assets dev:site",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "dev:site": "npx eleventy --serve",
    "prod": "npm-run-all del:dist del:assets build:assets build:site",
    "serve:prod": "serve ./dist/",
    "dev": "concurrently --kill-others \"npm run dev:assets\" \"npm run dev:site\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "fibers": "^5.0.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "mini-css-extract-plugin": "^1.3.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "serve": "^11.3.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  }
}
